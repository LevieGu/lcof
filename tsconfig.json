{
  "compilerOptions": {
    "target": "ES2021",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "./",
    "paths": {
      "@/src/" : ["./src/*"],
      "@/utils/*": ["./utils/*"],
      "@/types/*": ["./types/*"],
    }
  },
}